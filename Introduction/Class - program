package com.sarvesh.introduction;

public class Student {
    private int rollno;
    private String name;
    private float marks;

    public Student(int rollno, String name, float marks) {
        this.rollno = rollno;
        this.name = name;
        this.marks = marks;
    }

    public int getRollno() {
        return rollno;
    }

    public void setRollno(int rollno) {
        this.rollno = rollno;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "Student{" +
                "rollno=" + rollno +
                ", name='" + name + '\'' +
                ", marks=" + marks +
                '}';
    }

    public float getMarks() {
        return marks;
    }

    public void setMarks(float marks) {
        this.marks = marks;
    }
    public static void main(String[] args)
    {
        Student st=new Student(101,"sarvesh",97.8f);
//        st.rollno=101;
//        st.name="sarvesh";
//        st.marks=98.7;
//
        System.out.println(st.toString());
    }
}
